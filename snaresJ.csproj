<Project Sdk="Godot.NET.Sdk/4.5.1">
  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <TargetFramework Condition=" '$(GodotTargetPlatform)' == 'android' ">net9.0</TargetFramework>
    <EnableDynamicLoading>true</EnableDynamicLoading>
  </PropertyGroup>
  <ItemGroup>
    <Folder Include="script\components\" />
    <Folder Include="script\effects\" />
    <Folder Include="script\instantiator\" />
  </ItemGroup>
  <ItemGroup>
    <Compile Update="script\beatmaps\BeatmapEventData.cs">
      <DependentUpon>Beatmap.cs</DependentUpon>
    </Compile>
    <Compile Update="script\scene\GameInitialization.cs">
      <DependentUpon>Game.cs</DependentUpon>
    </Compile>
    <Compile Update="script\scene\GameProcessing.cs">
      <DependentUpon>Game.cs</DependentUpon>
    </Compile>
    <Compile Update="script\scene\GameState.cs">
      <DependentUpon>Game.cs</DependentUpon>
    </Compile>
  </ItemGroup>
</Project>